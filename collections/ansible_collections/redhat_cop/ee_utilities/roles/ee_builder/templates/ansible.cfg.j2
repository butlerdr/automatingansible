[galaxy]
server_list = automation_hub_cert,automation_hub_pub,automation_hub_comm
ignore_certs = yes

[galaxy_server.automation_hub_cert]
url={{ ah_host }}api/galaxy/content/rh-certified/
token={{ ah_token }}

[galaxy_server.automation_hub_pub]
url={{ ah_host }}api/galaxy/content/published/
token={{ ah_token }}

[galaxy_server.automation_hub_comm]
url={{ ah_host }}api/galaxy/content/community/
token={{ ah_token }}
